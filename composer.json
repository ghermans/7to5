{
    "name": "spatie/7to5",
    "description": "Convert PHP 7 code to PHP 5 code",
    "keywords": [
        "spatie",
        "7to5",
        "convert",
        "php7",
        "abstract",
        "syntax",
        "tree"
    ],
    "homepage": "https://github.com/spatie/7to5",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Van de Vreken",
            "homepage": "http://madewithlove.be/",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "nikic/php-parser": "^2.0",
        "symfony/console": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "illuminate/filesystem": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Php7to5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Php7to5\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
